<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitMenuButton?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.ColorAdjust?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.web.WebView?>
<TabPane xmlns:fx="http://javafx.com/fxml/1" id="AllPane" maxHeight="-Infinity" maxWidth="-Infinity"
  minHeight="-Infinity" minWidth="-Infinity" prefHeight="532.0" prefWidth="890.0"
  tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/11.0.14-internal"
  fx:controller="SceneBuilder">
  <tabs>
    <Tab text="Default Dictionary">
      <content>
        <AnchorPane blendMode="MULTIPLY" minHeight="0.0" minWidth="0.0" prefHeight="433.0"
          prefWidth="850.0">
          <children>
            <Button alignment="CENTER_RIGHT" id="success" layoutX="200.0"
              layoutY="31.0" mnemonicParsing="false" onMouseClicked="#dd_onClickButtonAddNewWord"
              prefHeight="44.0" prefWidth="134.0" text="Add new word"
              textAlignment="CENTER" fx:id="dd_buttonAddNewWord">
              <effect>
                <ColorAdjust/>
              </effect>
              <opaqueInsets>
                <Insets/>
              </opaqueInsets>
            </Button>
            <Button alignment="CENTER_RIGHT" id="showAll" layoutX="342.0"
              layoutY="31.0" mnemonicParsing="false" onMouseClicked="#dd_onClickButtonShowAllWords"
              prefHeight="44.0" prefWidth="143.0" text="Show all words"
              fx:id="dd_buttonShowAllWords"/>
            <Button alignment="CENTER_RIGHT" id="delete" layoutX="491.0"
              layoutY="31.0" mnemonicParsing="false" onMouseClicked="#dd_onClickButtonDeleteWord"
              prefHeight="44.0" prefWidth="147.0" text="Delete this word"
              fx:id="dd_buttonDeleteThisWord"/>
            <Button alignment="CENTER_RIGHT" id="change" layoutX="643.0"
              layoutY="31.0" mnemonicParsing="false" onMouseClicked="#dd_onClickButtonChangeWord"
              prefHeight="43.0" prefWidth="152.0" text="Change this word"
              fx:id="dd_buttonChangeThisWord"/>
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="744.0" layoutY="2.0"
              onMouseClicked="#dd_onClickFindIcon" pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@../icon/findIcon.png"/>
              </image>
            </ImageView>
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="795.0" layoutY="1.0"
              onMouseClicked="#dd_onClickDeleteIcon" pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@../icon/deleteIcon.png"/>
              </image>
            </ImageView>
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="770.0" layoutY="1.0"
              onMouseClicked="#dd_onClickListenIcon" pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@../icon/listenIcon.png"/>
              </image>
            </ImageView>
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="200.0" layoutY="41.0"
              pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@../icon/AddIcon.png"/>
              </image>
            </ImageView>
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="343.0" layoutY="41.0"
              pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@../icon/Untitled.png"/>
              </image>
            </ImageView>
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="491.0" layoutY="41.0"
              pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@../icon/deleteIcon.png"/>
              </image>
            </ImageView>
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="645.0" layoutY="40.0"
              pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@../icon/modify.png"/>
              </image>
            </ImageView>
            <ListView layoutY="31.0" onKeyPressed="#dd_onPushItemOfListViewSearcher"
              onMouseClicked="#dd_onPushItemOfListViewSearcher"
              prefHeight="463.0" prefWidth="200.0"
              fx:id="dd_listViewSearcher"/>
            <ListView layoutX="205.0" layoutY="186.0" prefHeight="308.0" prefWidth="676.0"
              fx:id="dd_listViewShowWord"/>
            <TextField layoutY="1.0" onKeyReleased="#dd_onChangeTextFieldFindWord"
              prefHeight="26.0" prefWidth="743.0" promptText="Type sth"
              fx:id="dd_textFieldFindWord"/>
            <VBox layoutX="204.0" layoutY="80.0" prefHeight="78.0" prefWidth="209.0"
              fx:id="dd_vBoxAddNewWord">
              <children>
                <Button id="success" mnemonicParsing="false" onMouseClicked="#dd_onClickButtonSubmitAddNewWord"
                  prefHeight="27.0" prefWidth="96.0"
                  text="Submit" fx:id="dd_buttonSubmitAddNewWord"/>
                <TextField prefHeight="25.0" prefWidth="204.0" promptText="Vietnamese"
                  fx:id="dd_textFieldAddVietnamese"/>
                <TextField prefHeight="25.0" prefWidth="203.0" promptText="English"
                  fx:id="dd_textFieldAddEnglish"/>
              </children>
            </VBox>
            <VBox layoutX="565.0" layoutY="83.0" prefHeight="82.0" prefWidth="232.0"
              fx:id="dd_vBoxChangeThisWord">
              <children>
                <Button id="success" minWidth="44.0" mnemonicParsing="false"
                  onMouseClicked="#dd_onClickButtonSubmitChangeThisWord" prefHeight="27.0"
                  prefWidth="62.0" text="Submit" fx:id="dd_buttonSubmitChangeWord">
                  <font>
                    <Font/>
                  </font>
                </Button>
                <TextField prefHeight="26.0" prefWidth="215.0" promptText="New mean of this word"
                  fx:id="dd_textFieldChangeMeanOfWord"/>
              </children>
            </VBox>
          </children>
        </AnchorPane>
      </content>
    </Tab>
    <Tab onSelectionChanged="#gg_changeTabToMultiDictionary" text="Multi Dictionary">
      <content>
        <AnchorPane>
          <children>
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="527.0" layoutY="2.0"
              onMouseClicked="#md_onClickFindIcon" pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@../icon/findIcon.png"/>
              </image>
            </ImageView>
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="597.0" layoutY="1.0"
              onMouseClicked="#md_onClickDeleteIcon" pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@../icon/deleteIcon.png"/>
              </image>
            </ImageView>
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="562.0" layoutY="1.0"
              onMouseClicked="#md_onClickListenIcon" pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@../icon/listenIcon.png"/>
              </image>
            </ImageView>
            <ListView layoutY="51.0" onMouseClicked="#md_onPushItemOfListViewSearcher"
              prefHeight="446.0" prefWidth="159.0"
              fx:id="md_listViewSearcher"/>
            <SplitMenuButton layoutX="632.0" layoutY="-1.0"
              mnemonicParsing="false" prefHeight="24.0" prefWidth="247.0" styleClass="split-menu-btn-info"
              text="Oxford Advanced Learner's Dictionary" fx:id="md_splitMenuButtonChooseDictionary">
              <graphic>
                <ImageView fitHeight="23.0" fitWidth="23.0" pickOnBounds="true"
                  preserveRatio="true">
                  <image>
                    <Image url="@../icon/selectIcon.png"/>
                  </image>
                </ImageView>
              </graphic>
              <items>
                <MenuItem mnemonicParsing="false" text="Oxford Advanced Learner's Dictionary"
                  fx:id="dict0">
                  <graphic>
                    <ImageView pickOnBounds="true" preserveRatio="true">
                      <image>
                        <Image
                          url="@../mdict/Oxford%20Advanced%20Learner's%20Dictionary/Oxford%20Advanced%20Learner's%20Dictionary.png"/>
                      </image>
                    </ImageView>
                  </graphic>
                </MenuItem>
                <MenuItem mnemonicParsing="false" text="Cambridge Advanced Learner Dictionary 4"
                  fx:id="dict2">
                  <graphic>
                    <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true"
                      preserveRatio="true">
                      <image>
                        <Image
                          url="@../mdict/Cambridge%20Advanced%20Learner/Cambridge%20Advanced%20Learner%20Dictionary%204.png"/>
                      </image>
                    </ImageView>
                  </graphic>
                </MenuItem>
              </items>
            </SplitMenuButton>
            <TextField layoutY="1.0" onKeyReleased="#md_onChangeTextFieldFindWord"
              prefHeight="28.0" prefWidth="520.0" promptText="Type sth"
              fx:id="md_textFieldFindWord"/>
            <WebView layoutX="173.0" layoutY="51.0" prefHeight="446.0" prefWidth="707.0"
              fx:id="md_webView"/>
          </children>
        </AnchorPane>
      </content>
    </Tab>
    <Tab onSelectionChanged="#gg_changeTabToGoogleTranslate" text="Google Translate">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
          <children>
            <WebView layoutX="3.0" layoutY="9.0" prefHeight="490.0" prefWidth="882.0"
              fx:id="gg_webView"/>
          </children>
        </AnchorPane>
      </content>
    </Tab>
  </tabs>
</TabPane>
